<div class="panel panel-default">
  <div class="panel-heading">REVIEWS</div>
  <div class="panel-body">
    <% @microposts.each do |micropost| %>
    <div class="well pull-left">
      <ul class="media-list ">
        <li><%= image_tag micropost.picture.url if micropost.picture? %>
        <li><%= link_to micropost.user.name, micropost.user %>
        <li><%= micropost.content %>
        <li><%= micropost.stars %>
      <li id="like-of-<%= micropost.id %>">
        <%= render 'likes/like', micropost: micropost %>
      </li>
    </div>
    <% end %>
  </div>
</div>
